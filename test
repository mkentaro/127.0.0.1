#include<stdio.h>
#include<string.h>
#define CHAIR_NUM 10

int ChairPair[CHAIR_NUM][2]={{0,0},{3,2},{1,4},{5,1},{2,6},{7,3},{4,8},{9,5},{6,9},{8,7}};
char ChairStatus[CHAIR_NUM];
char Input[20]="0NABETanI";
//char Input[10]="0ABC";


int check(int input_num){
  int chair_num;

  if(Input[input_num]>='a' && Input[input_num]<='z'){
    for( chair_num=1; chair_num<CHAIR_NUM; chair_num++){
      if( ChairStatus[chair_num]==(Input[input_num]-32)){
        ChairStatus[chair_num]='-';
        return 0;
      }
    }
  }


  for( chair_num=1; chair_num<CHAIR_NUM; chair_num++){
    if( ChairStatus[chair_num]=='-' && ChairStatus[ChairPair[chair_num][0]]=='-' && ChairStatus[Ch\
airPair[chair_num][1]]=='-'){
      ChairStatus[chair_num]=Input[input_num];
      return 0;
    }
  }

  for( chair_num=1; chair_num<CHAIR_NUM; chair_num++){
    if( ChairStatus[chair_num]=='-' && ( ChairStatus[ChairPair[chair_num][0]]=='-' || ChairStatus[\
ChairPair[chair_num][1]]=='-')){
      ChairStatus[chair_num]=Input[input_num];
      return 0;
    }
  }

  for( chair_num=1; chair_num<CHAIR_NUM; chair_num++){
    if( ChairStatus[chair_num]=='-'){
      ChairStatus[chair_num]=Input[input_num];
      return 0;
    }
  }

}

int main(){
  int InputLength=strlen(Input);
    printf("InputLength=%d\n", InputLength);
  int input_num, chair_num;

  for( chair_num=1; chair_num<CHAIR_NUM; chair_num++){
    ChairStatus[chair_num]='-';
    //printf("[%d]=%d, %d\n", i, ChairPair[i][0], ChairPair[i][1]);
    //printf("%c", ChairStatus[chair_num]);
  }

  for( chair_num=1; chair_num<CHAIR_NUM; chair_num++){
    printf("%c", ChairStatus[chair_num]);
  }

  printf("\n");

  for( input_num=1; input_num<InputLength; input_num++){
    check(input_num);

  }

  printf("\n");
  for( chair_num=1; chair_num<CHAIR_NUM; chair_num++){
    printf("%c", ChairStatus[chair_num]);
  }
  printf("\n");


}
